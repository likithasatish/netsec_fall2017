#!/root/netsec_fall2017/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'playground==0.3.0','console_scripts','launch_vnic'
__requires__ = 'playground==0.3.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('playground==0.3.0', 'console_scripts', 'launch_vnic')()
    )
